
{% if links|length > 0 %}
  <ul class="menu vertical">
    {% for link in links %}
      {% if link['menu-text'] %}
        <li class="menu-text">{{ link['menu-text'] }}</li>
      {% else %}
        <li><a href="{{ link['link'] }}" target="{{ link['target'] }}">{{ link['text'] }}</a></li>
      {% endif %}
    {% endfor %}
  </ul>
{% endif %}
